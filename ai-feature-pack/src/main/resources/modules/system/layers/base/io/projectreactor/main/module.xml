<module name="io.projectreactor" xmlns="urn:jboss:module:1.9">

    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <resources>
        <artifact name="${io.projectreactor:reactor-core}"/>
        <artifact name="${io.projectreactor.netty:reactor-netty-core}"/>
        <artifact name="${io.projectreactor.netty:reactor-netty-http}"/>
    </resources>

    <dependencies>
        <module name="io.netty.netty-buffer" export="true"/>
        <module name="io.netty.netty-codec" export="true"/>
        <module name="io.netty.netty-codec-dns" export="true"/>
        <module name="io.netty.netty-codec-http" export="true"/>
        <module name="io.netty.netty-codec-http2" export="true"/>
        <module name="io.netty.netty-codec-socks" export="true"/>
        <module name="io.netty.netty-common" export="true"/>
        <module name="io.netty.netty-handler" export="true"/>
        <module name="io.netty.netty-handler-proxy" export="true"/>
        <module name="io.netty.netty-resolver" export="true"/>
        <module name="io.netty.netty-resolver-dns" export="true"/>
        <module name="io.netty.netty-transport" export="true"/>
        <module name="io.netty.netty-transport-native-unix-common" export="true"/>
        <module name="io.netty.netty-transport-native-epoll" export="true"/>
        <module name="io.netty.netty-transport-native-kqueue" export="true"/>
        <module name="org.slf4j"/>
        <module name="org.reactivestreams" export="true"/>
    </dependencies>
</module> 